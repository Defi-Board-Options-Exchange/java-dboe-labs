module com.ngontro86.server.dboe_lockedbalance;

create schema DboeLockedBalanceEvent as (
    wallet_id string,
    instr_id string,
    buy_sell int,
    user_order_id string,
    amount double,
    in_timestamp long
);

create window DboeLockedBalanceWin.std:unique(wallet_id, instr_id, buy_sell, user_order_id) as DboeLockedBalanceEvent;
@Name('DboeLockedBalanceWin') insert into DboeLockedBalanceWin select * from DboeLockedBalanceEvent;